        <servlet-name>3</servlet-name>
        <url-pattern>/BServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>4</servlet-name>
        <servlet-class>cn.java.web.servlet.ProvinceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>4</servlet-name>
